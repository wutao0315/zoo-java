<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:zoo="http://www.wxius.com/schema/zoo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://www.wxius.com/schema/zoo http://www.wxius.com/schema/zoo.xsd">
    <zoo:config />

    <bean class="com.wxius.framework.zoo.spring.XmlConfigPlaceholderAutoUpdateTest.TestXmlBeanWithConstructorArgs">
        <constructor-arg index="0" value="${timeout}"/>
        <constructor-arg index="1" value="${batch}"/>
    </bean>
</beans>
